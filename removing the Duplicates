import java.util.*;

public class RemoveDuplicates{
    public static Integer[] removeDuplicatesUsingSet(Integer[] arr){
        Set<Integer> set = new HashSet<>(Arrays.asList(arr));
        return set.toArray(new Integer[0]);
        
    }
    
    public static void main (String []args){
        Integer[] arr = {1,2,3,3,4,5,6,6,7,7,8,9,10,10};
        System.out.println("Original Array: " + Arrays.toString(arr));
        Integer[] result = removeDuplicatesUsingSet(arr);
        System.out.println("Array after removing the duplicates: " + Arrays.toString(result));
    }
}
